{
    "$schema": "https://www.krakend.io/schema/krakend.json",
    "version": 3,
    "name": "Project000 API Gateway",
    "timeout": "3000ms",
    "cache_ttl": "300s",
    "output_encoding": "json",
    "port": 8080,
    "host": [
        "http://127.0.0.1:8080",
        "http://project000-backend-recommend:8080"
    ],
    "debug_endpoint": true,
    "extra_config": {
        "telemetry/logging": {
            "level": "DEBUG",
            "prefix": "[KRAKEND]",
            "syslog": false,
            "stdout": true
